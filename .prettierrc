{
  "arrowParens": "always",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "printWidth": 120,
  "quoteProps": "as-needed",
  "semi": true,
  "jsxSingleQuote": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",

  "overrides": [
    {
      "files": "*.tsx, *.ts",
      "options": { "parser": "babel" }
    },
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    }
  ]
}
